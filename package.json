    "@storybook/addons": "6.1.21",
    "@storybook/react": "6.1.21",
    "@types/jest": "26.0.22",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "babel-loader": "8.2.2",
    "docz": "2.3.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-jest": "24.3.2",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "5.2.0",
    "np": "7.4.0",
    "pinst": "2.1.6",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-is": "17.0.1",
    "size-limit": "4.10.1",
    "tsdx": "0.14.1",
    "tslib": "2.1.0",
    "typescript": "4.2.3"
  },
  "dependencies": {
    "@material-ui/icons": "^4.11.2",
