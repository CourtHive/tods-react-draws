  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "tods-react-draws",
  "author": "Charles Allen",
  "module": "dist/tods-react-draws.esm.js",
  "devDependencies": {
    "@babel/core": "7.15.5",
