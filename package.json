    "@types/jest": "27.0.1",
    "@types/react": "17.0.21",
    "@types/react-dom": "17.0.9",
    "babel-jest": "27.2.0",
    "babel-loader": "8.2.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "24.4.2",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.2",
    "np": "7.5.0",
    "pinst": "2.1.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "size-limit": "5.0.3",
    "tsdx": "0.14.1",
    "tslib": "2.3.1",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "@material-ui/core": "4.12.3",
